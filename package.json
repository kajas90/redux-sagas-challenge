{
  "name": "react-quickstart",
  "version": "4.3.0",
  "description": "React starter kit for single page web applications.",
  "scripts": {
    "preremove-demo": "npm run remove-dist",
    "remove-demo": "babel-node tools/removeDemo.js",
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message remove-dist",
    "start": "npm-run-all --parallel  open:src lint:watch test:watch",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint": "esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm-run-all clean-dist test lint",
    "build": "babel-node tools/build.js && npm run open:dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/blurbyte/react-quickstart"
  },
  "keywords": [
    "react",
    "redux",
    "react-router",
    "quickstart",
    "jest",
    "webpack2",
    "redux-saga"
  ],
  "author": "blurbyte",
  "license": "MIT",
  "engines": {
    "node": ">=6"
  },
  "jest": {
    "testEnvironment": "node",
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "coverageDirectory": "./coverage/"
  },
  "dependencies": {
    "history": "4.6.1",
    "promise-polyfill": "6.0.2",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-helmet": "5.1.3",
    "react-redux": "5.0.5",
    "react-router-dom": "4.1.1",
    "react-router-redux": "v5.0.0-alpha.6",
    "redux": "3.6.0",
    "redux-saga": "0.15.3",
    "sanitize.css": "5.0.0",
    "styled-components": "2.1.0",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.0.0",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.3",
    "babel-plugin-transform-regenerator": "6.24.1",
    "babel-preset-env": "1.5.2",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "6.24.1",
    "browser-sync": "2.18.12",
    "chalk": "1.1.3",
    "compression": "1.6.2",
    "connect-history-api-fallback": "1.3.0",
    "css-loader": "0.28.4",
    "eslint": "4.1.1",
    "eslint-import-resolver-webpack": "0.8.3",
    "eslint-plugin-import": "2.3.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "7.1.0",
    "eslint-watch": "3.1.1",
    "exports-loader": "0.6.4",
    "express": "4.15.2",
    "file-loader": "0.11.2",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.3",
    "jest-styled-components": "3.0.2",
    "npm-run-all": "4.0.1",
    "offline-plugin": "4.8.1",
    "open": "0.0.5",
    "prop-types": "15.5.10",
    "react-hot-loader": "3.0.0-beta.6",
    "react-test-renderer": "15.6.1",
    "redux-immutable-state-invariant": "2.0.0",
    "regenerator": "0.9.7",
    "rimraf": "2.5.4",
    "style-loader": "0.18.2",
    "url-loader": "0.5.9",
    "webpack": "3.0.0",
    "webpack-dev-middleware": "1.11.0",
    "webpack-hot-middleware": "2.18.0",
    "webpack-md5-hash": "0.0.5"
  }
}
